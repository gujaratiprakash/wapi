{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "uploads/*",
        "use": "@vercel/static"
      },
      {
        "src": "sessions/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/send-message",
        "methods": ["POST"],
        "dest": "/api/index.js"
      },
      {
        "src": "/send-media",
        "methods": ["POST"],
        "dest": "/api/index.js"
      }
    ]
  }
  